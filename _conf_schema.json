    "song_limit": {
        "description": "搜索歌曲数量限制",
        "hint": "搜索歌曲数量限制",
        "type": "int",
        "slider": {
            "min": 1,
            "max": 20,
            "step": 1
        },
        "default": 5
    },
    "select_mode": {
        "description": "选择模式",
        "hint": "此配置决定搜索歌曲的展示方法：文本模式(text)、 图片模式(image)、 单曲模式(single)。 注意单曲模式下将直接默认选取第一首歌曲",
        "type": "string",
        "options": [
            "text",
            "image",
            "single"
        ],
        "default": "text"
    },
    "send_mode": {
        "description": "发送模式",
        "hint": "此配置决定发送歌曲的方式: 卡片模式(card)、语音模式(record)、文本模式(text), 注意卡片模式仅QQ发送网易云音乐时可用, 语音模式失败率较高且发送很慢， 文本模式将以链接形式发送",
        "type": "string",
        "options": [
            "text",
            "record",
            "card"
        ],
        "default": "card"
    },
    "enable_comments": {
        "description": "是否启用评论",
        "hint": "开启后在发送歌曲后附加一条评论",
        "type": "bool",
        "default": true
    },
    "enable_lyrics": {
        "description": "是否启用歌词",
        "hint": "开启后在发送歌曲后附加一张歌曲图片",
        "type": "bool",
        "default": false
    },
    "proxy": {
        "description": "代理地址",
